# -*- mode: snippet -*-
# name: subfigure (subfig)
# key: subfig
# group: environments
# expand-env: ((yas-indent-line 'fixed))
# --
\begin{figure}[htbp]
  \centering
  \subfloat[subfig caption]{
    \includegraphics[width=.48\textwidth]{}
    \label{subfig:label}
  }
  ~ % \\\\, \quad, \hfill, etc.
  \subfloat[subfig caption]{
    \includegraphics[width=.48\textwidth]{}
    \label{subfig:label}
  }
  \caption{$0}
  \label{fig:label}
\end{figure}
